{
    "name": "03-solid",
    "version": "1.0.0",
    "description": "GymPass style app",
    "main": "index.js",
    "scripts": {
        "start:dev": "tsx watch src/server.ts",
        "build": "tsup src --out-dir build",
        "start": "node build/server.js",
        "lint": "eslint src --ext .ts --fix",
        "pretest:e2e": "run-s teste:create-prisma-environment test:install-prisma-environment",
        "test": "vitest run --dir src/services",
        "teste:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
        "test:install-prisma-environment": "npm link vitest-environment-prisma",
        "test:e2e": "vitest run --dir src/http",
        "test:watch": "vitest --dir src/services",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "kill-db-port": "sudo ss --kill state listening src :5432",
        "delete-container": "docker compose down",
        "delete-db": "docker system prune -a",
        "create-db": "docker compose up -d",
        "apply-migrations": "npx prisma migrate dev",
        "initialize-prisma-studio": "npx prisma studio",
        "initialize-db": "run-s kill-db-port delete-container delete-db create-db",
        "initialize-prisma": "run-s apply-migrations initialize-prisma-studio"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@rocketseat/eslint-config": "2.1.0",
        "@types/node": "20.5.7",
        "@vitest/coverage-v8": "0.34.3",
        "@vitest/ui": "0.34.3",
        "eslint": "8.48.0",
        "npm-run-all": "4.1.5",
        "prisma": "5.2.0",
        "supertest": "6.3.3",
        "tsup": "7.2.0",
        "tsx": "3.12.7",
        "typescript": "5.2.2",
        "vite-tsconfig-paths": "4.2.0",
        "vitest": "0.33.0"
    },
    "dependencies": {
        "@fastify/jwt": "7.2.1",
        "@prisma/client": "5.2.0",
        "@types/bcryptjs": "2.4.3",
        "bcryptjs": "2.4.3",
        "dayjs": "1.11.9",
        "dotenv": "16.3.1",
        "fastify": "4.22.0",
        "zod": "3.22.2"
    }
}
